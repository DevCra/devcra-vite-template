{
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": false,
  "printWidth": 100,
  "trailingComma": "all",
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "lf",
  "arrowParens": "always",
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/pages/(.*)$",
    "^@/pageComponents/(.*)$",
    "^@/components/(.*)$",
    "^@/contexts/(.*)$",
    "^@/stores/(.*)$",
    "^@/hooks/(.*)$",
    "^@/storage/(.*)$",
    "^@/remotes/(.*)$",
    "^@/constants/(.*)$",
    "^@/libs/(.*)$",
    "^@/utils/(.*)$",
    "^@/assets/(.*)$",
    "^@/mocks/(.*)$",
    "^@/tests/(.*)$",
    "^@/types/(.*)$",
    "^@/styles/(.*)$",
    "^[./]"
  ],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
